{
  "name": "openapi",
  "version": "1.0.0",
  "description": "koa openapi framework",
  "main": "server.js",
  "scripts": {
    "test": "mocha test/test.js",
    "lint": "eslint ."
  },
  "keywords": [
    "koa",
    "openapi"
  ],
  "author": "monsterwater",
  "license": "MIT",
  "dependencies": {
    "co": "^4.6.0",
    "koa": "^0.21.0",
    "koa-bodyparser": "^2.0.0",
    "koa-cors": "0.0.16",
    "koa-response-time": "^1.0.2",
    "memory-cache": "^0.1.4",
    "mysql-co": "0.0.4",
    "underscore": "^1.8.3",
    "debug": "*",
    "netrc": "~0.1.3",
    "write-to": "^1.0.0",
    "raw-body": "^1.1.2",
    "statuses": "^1.0.2"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "cheerio": "^0.19.0",
    "co-mocha": "^1.1.2",
    "co-supertest": "0.0.10",
    "eslint": "^0.24.0",
    "mocha": "^2.2.5",
    "nodemon": "^1.3.7",
    "npm-check": "^3.2.10",
    "npm-check-updates": "^1.5.1",
    "supertest": "^1.0.1"
  },
  "eslintConfig": {
    "env": {
      "mocha": true,
      "node": true,
      "es6": true
    },
    "rules": {
      "comma-dangle": [
        2,
        "always-multiline"
      ],
      "curly": [
        2,
        "multi-line"
      ],
      "eqeqeq": 0,
      "quotes": [
        2,
        "single",
        "avoid-escape"
      ],
      "generator-star": [
        2,
        "start"
      ],
      "key-spacing": 0,
      "new-cap": [
        2,
        {
          "capIsNew": true,
          "capIsNewExceptions": [
            "ModelError"
          ]
        }
      ],
      "no-multi-spaces": 0,
      "no-underscore-dangle": 0,
      "no-shadow": 1,
      "space-infix-ops": 0
    }
  }
}
